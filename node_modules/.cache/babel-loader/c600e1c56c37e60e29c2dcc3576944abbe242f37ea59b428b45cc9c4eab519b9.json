        // sessionStorage.setItem(\"displayName\",user.user.displayName)\r\n          if(user.user.refreshToken){\r\n            this.$store.commit(\"loginToken\", {refreshToken:user.user.refreshToken, uid:user.user.uid})\r\n          }\r\n\r\n          this.$router.replace('/')\r\n          window.scrollTo({top:0,behavior:'auto'})\r\n          alert('thank You')\r\n          // 로그인되면 메인페이지로 이동\r\n          // 로그인 되면서 바로 홈 페이지 제일 상단으로 이동\r\n          // behavior 를 smooth 로 하면 스크롤 효과가 나옴.\r\n        }),\r\n        ((error)=>{\r\n          this.errorMsg=error.message\r\n        })\r\n        )\r\n      }\r\n      \r\n    }\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"],"mappings":"AAoBA,SAAQA,IAAI,QAAO,aAAY;AAC3B,eAAe;EACXC,IAAI,EAAC,WAAW;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAC,EAAE;MACRC,QAAQ,EAAC;MACT;IACJ;EACJ,CAAC;;EACDC,OAAO,EAAE;IACXC,KAAKA,CAAA,EAAE;MACL,IAAG,CAAC,IAAI,CAACH,KAAK,EAAC;QACbI,KAAK,CAAC,aAAa,CAAC;QACpB,OAAO,KAAI;QACX;MACF;;MACA,IAAG,CAAC,IAAI,CAACH,QAAQ,EAAC;QAChBG,KAAK,CAAC,cAAc,CAAC;QACrB,OAAO,KAAK;MACd;MACAP,IAAI,CAACQ,0BAA0B,CAAC,IAAI,CAACL,KAAK,EAAC,IAAI,CAACC,QAAQ,CAAC,CAACK,IAAI,CAAGC,IAAI,IAAG;QACtEC,OAAO,CAACC,GAAG,CAACF,IAAI,CAACA,IAAI;QACrB;QACA;QACA,IAAGA,IAAI,CAACA,IAAI,CAACG,YAAY,EAAC;UACxB,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,YAAY,EAAE;YAACF,YAAY,EAACH,IAAI,CAACA,IAAI,CAACG,YAAY;YAAEG,GAAG,EAACN,IAAI,CAACA,IAAI,CAACM;UAAG,CAAC;QAC3F;QAEA,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,GAAG;QACxBC,MAAM,CAACC,QAAQ,CAAC;UAACC,GAAG,EAAC,CAAC;UAACC,QAAQ,EAAC;QAAM,CAAC;QACvCf,KAAK,CAAC,WAAW;QACjB;QACA;QACA;MACF,CAAC,EACCgB,KAAK,IAAG;QACR,IAAI,CAACC,QAAQ,GAACD,KAAK,CAACE,OAAM;MAC5B,CACA;IACF;EAEF;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}